          <stringProp name="parameters">16</stringProp>
          <stringProp name="script">/**
 *  Скрипт извлекает список всех основных разделов содержания. страницы
 *  (https://habr.com/ru/post/349860/)
 *  и выводит полученные данные в виде ответа JSR223-сэмплера
 *  в формате —   №  :  Название_раздела 
 * 
 *  return(result)  —  для визуального контроля через Debug Sampler
 */


// Обозначаем в логах старт скрипта
log.info(&quot;START ScraplerRegex Script...&quot;)

// Формируем контекст
target_content = vars.get(&quot;target_content_1&quot;)  // Boundary Targetl_content
check_count = Integer.parseInt(args[0])

// Ищем все основные разделы содержания
elements = target_content =~ /(?su)(?&lt;=&lt;b&gt;).*?(?=&lt;\/b&gt;)/
count = elements.size()

// Проверяем получилось ли найти все разделы
if (count == check_count) {
	log.info(&quot;OK, elements.size() вернуло ${count} — найдены все основные разделы содержания.&quot;)
} else {
     throw new RuntimeException(&quot;Oops, elements.size() вернуло ${count}, а ожидалось ${check_count} !&quot;)
}


result = new StringBuilder()
result.append(String.format(&quot;  %-3s:   %-59s%n&quot;, &quot;№&quot;, &quot;Название_раздела&quot;))
/*   В цикле получаем имя раздела содержания,
 *   и добавляем к результирующей строке.
 */
for (i = 1; i &lt;= count; i++) {
	if (i &lt; 10) {c = &quot; &quot;} else {c = &quot;&quot;}
	
	element = elements.getAt(i-1)
	
	result.append(&quot;  ${-&gt;c}${i}${-&gt;c}   :    ${element}\n&quot;)
	log.info(&quot;element_$i = $element&quot;)
}

// Обозначаем в логах завершение скрипта
log.info(&quot;END ScraplerRegex Script.&quot;)
// Для визуального контроля через Debug Sampler возвращаем result
return(result)</stringProp>
