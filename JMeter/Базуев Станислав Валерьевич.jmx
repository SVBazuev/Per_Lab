          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor">
            <stringProp name="cacheKey">false</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters">16 PostProcessor_Content_length txt</stringProp>
            <stringProp name="script">/**
 *  Скрипт извлекает данные из html страницы
 *  и записывает файл (&quot;PostProcessor_Content_length.txt&quot;) с данными в формате:
 *  		Порядковый_номер_раздела : Название_раздела : Объем_раздела
 * 
 *  return(0)
 */


// Обозначаем в логах старт скрипта
log.info(&quot;START PostProcessor Script...&quot;)

// Формируем контекст
check_count = Integer.parseInt(args[0])
responseBody = new String(prev.getResponseDataAsString())
//  файл для сохранения результата
baseDir = new File(org.apache.jmeter.services.FileServer.getFileServer().getBaseDir())
			.getAbsolutePath()
fileSep = File.separator
fileName = &quot;${args[1]}.${args[2]}&quot;
absPathFile = /${baseDir}${fileSep}${-&gt;fileName}/
log.info(&quot;Результат будет сохранён по пути:\n${absPathFile}&quot;)
file = new File(&quot;${-&gt;absPathFile}&quot;)
// Подготовка паттерна
f = &quot;(?su)&quot;                             //  модификаторы (флаги) регулярок
el = &quot;&quot;                                 //  переменный элемент 
h = &quot;(?&lt;=&lt;h2&gt;${-&gt;el}&lt;/h2&gt;)&quot;             //  head (позитивный просмотр назад)
exp = &quot;.*?&quot;                             //  expression (выражение)
t = &quot;(?=&lt;h2&gt;)&quot;                          //  tail (позитивный просмотр вперёд)
pattern = /${f}${-&gt;h}${-&gt;exp}${-&gt;t}/    //  паттерн в сборе

// Ищем все основные разделы содержания
elements = responseBody =~ /${f}(?&lt;=&lt;b&gt;).*?(?=&lt;\/b&gt;)/
count = elements.size()

// Проверяем получилось ли найти все разделы
if (count == check_count) {
	log.info(&quot;OK, elements.size() вернуло ${count} — найдены все основные разделы содержания.&quot;)
} else {
     throw new RuntimeException(&quot;Oops, elements.size() вернуло ${count}, а ожидалось ${check_count} !&quot;)
}

/*   В цикле получаем имя раздела содержания,
 *   корректируем паттерн и извлекаем содержимое раздела.
 *   Далее нормализуем содеримое (удаляя теги) и считаем длинну,
 *   построчно записываем результаты в файл.
 */
for (i = 1; i &lt;= count; i++) {
	if (i == count) {
		t = &quot;(?=&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;!----&gt;&lt;!----&gt;&lt;/div&gt;)&quot;
	}
	
	element = elements.getAt(i-1)
	log.info(&quot;element_$i = $element&quot;)
	el = element

	if (el == null || el.trim().isEmpty()) {
		throw new RuntimeException(&quot;Element name is null or empty!&quot;)
	}
    
	if (el.contains(&quot;...)&quot;)) {
		// Дополняем элемет тегами и экранируем
		if (el.contains(&quot;(?:...)&quot;)) { 
			el = el.replace(&quot;(?:...)&quot;, &quot;&lt;code&gt;\\\\(\\\\?:\\\\.\\\\.\\\\.\\\\)&lt;/code&gt;&quot;)
					.replace(&quot;|&quot;, &quot;&lt;code&gt;\\\\|&lt;/code&gt;&quot;)
		} else {
		    el = el.replace(&quot;(...)&quot;, &quot;&lt;code&gt;\\\\(\\\\.\\\\.\\\\.\\\\)&lt;/code&gt;&quot;)
		            .replace(&quot;match&quot;, &quot;&lt;code&gt;match&lt;/code&gt;&quot;)
		}
	}
	
	m = responseBody =~ /${pattern}/
    
	if (!m) {
		throw new RuntimeException(&quot;Oops, не получилось найти соотвествие паттерну!&quot;)
	} else {
		// Удаление вложенных тегов
		extra_m = m.group(0).replaceAll(/&lt;.*?&gt;/, &apos;&apos;)

		line = &quot;${i} : ${element} : ${extra_m.length()}&quot;
		if (i == 1) {
			file.withWriter(&apos;UTF-8&apos;) { writer -&gt;
				writer.writeLine(&quot;${line}&quot;)
			}
		} else {
			file.withWriterAppend(&apos;UTF-8&apos;) { writer -&gt;
				writer.writeLine(&quot;${line}&quot;)
			}
		}
	}
}
// Обозначаем в логах завершение скрипта
log.info(&quot;... END PostProcessor Script&quot;)

return(0)</stringProp>
