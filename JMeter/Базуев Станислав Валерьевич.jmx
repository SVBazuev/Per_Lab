            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 ExFor chooseFlight">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters">50</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">/**
 *  Скрипт извлекает данные из html страницы
 *  и создаёт в области JMeter следующие переменные:
 *      —  &apos;advanceDiscount&apos; ;
 *      —  &apos;depart&apos; ;
 *      —  &apos;departDate&apos; ;
 *      —  &apos;arrive&apos; ;
 *      —  &apos;roundtrip&apos; ;
 *      —  &apos;returnDate&apos; ;
 *      —  &apos;numPassengers&apos; ;
 *      —  &apos;seatPref&apos; ;
 *      —  &apos;seatType&apos; .
 *  Параметры:
 *            args[0]: процент покупки билета в один конец,  
 *                     если 100 — всегда в одном направлении ; 
 *                     если 0  — всегда туда и обратно ; 
 *                     если 75  — в 25% случаев туда и обратно. 
 * 
 *  return(0)
 */


import java.util.Random
import org.jsoup.Jsoup
import org.jsoup.nodes.Document
import java.time.LocalDate
import java.time.format.DateTimeFormatter


// Обозначаем в логах старт скрипта
log.info(&quot;START PostProcessor Script...&quot;)

// Формируем контекст
responseBody = new String(prev.getResponseDataAsString())

docJsoup = Jsoup.parse(responseBody)

requestData = [
	&apos;advanceDiscount&apos;: &quot;&quot;,
	&apos;depart&apos;:	&quot;&quot;,
	&apos;departDate&apos;: &quot;&quot;,
	&apos;arrive&apos;: &quot;&quot;,
	&apos;roundtrip&apos;: &quot;&quot;, //[&quot;on&quot;, &quot;off&quot;],
	&apos;returnDate&apos;: &quot;&quot;,
	&apos;numPassengers&apos;: &quot;&quot;,
	&apos;seatPref&apos;: &quot;&quot;,  // можно было хардкодить [&quot;Aisle&quot;, &quot;Window&quot;, &quot;None&quot;],
	&apos;seatType&apos;: &quot;&quot;,  // можно было хардкодить [&quot;First&quot;, &quot;Business&quot;, &quot;Coach&quot;],
]

random = new Random()
formatter = DateTimeFormatter.ofPattern(&quot;MM/dd/yyyy&quot;)
buffer_departDate = &quot;&quot;
for (key in requestData.keySet()) {
	log.info(&quot;key: ${key}&quot;)

	switch (key) {
		case &apos;advanceDiscount&apos;:
			result = docJsoup.select(&quot;input[name=&apos;advanceDiscount&apos;]&quot;).attr(&quot;value&quot;)
			log.info(&quot;body.input.get(&apos;value&apos;) = ${result}&quot;)
			requestData[key] = result
//			vars.put(key, result)
			break
		case &apos;depart&apos;:
			cities = docJsoup.select(&quot;select[name=&apos;depart&apos;] option&quot;)
						.collect { it.attr(&quot;value&quot;) }
			if (cities.isEmpty()) {
				log.warn(&quot;Oops, в depart нет доступных городов для выбора.&quot;)
				break
			}
			// Выбираем случайный город вылета из списка
			result = cities[random.nextInt(cities.size())]
			log.info(&quot;depart = ${result}&quot;)
			requestData[key] = result
//			vars.put(key, result)
			break
		case &apos;departDate&apos;:
			result = docJsoup.select(&quot;input[name=&apos;departDate&apos;]&quot;).attr(&quot;value&quot;)
			log.info(&quot;Исходная дата вылета departDate = ${result}&quot;)

			departDate = LocalDate.parse(result, formatter)
			daysToAdd = random.nextInt(91)
			newDepartDate = departDate.plusDays(daysToAdd)
			newDepartDate = newDepartDate.format(formatter)
			log.info(&quot;newDepartDate = ${newDepartDate}&quot;)
			buffer_departDate = newDepartDate
			requestData[key] = newDepartDate
//			vars.put(key, newDepartDate)
			break
		case &apos;arrive&apos;:
			cities = docJsoup.select(&quot;select[name=&apos;arrive&apos;] option&quot;)
						.collect { it.attr(&quot;value&quot;) }
			if (cities.isEmpty()) {
				log.warn(&quot;Oops, в arrive нет доступных городов для выбора.&quot;)
				break
			}
			// Выбираем случайный город, но отличный от города вылета
			do {
				result = cities[random.nextInt(cities.size())]
			} while (result == requestData[&apos;depart&apos;])
			
			log.info(&quot;arrive = ${result}&quot;)
			requestData[key] = result
//			vars.put(key, result)
			break
		case &apos;roundtrip&apos;:
			chance = random.nextInt(100)
			procent = Integer.parseInt(args[0])
			if (procent == 100) {
				result = &quot;off&quot;
			} else if (procent == 0) {
				result = &quot;on&quot;
			} else {
				result = chance &gt;= procent ? &quot;on&quot; : &quot;off&quot;
			}
			
			log.info(&quot;roundtrip = ${result}&quot;)
			requestData[key] = result
//			vars.put(key, result)
			break
		case &apos;returnDate&apos;:
			if (requestData[&apos;roundtrip&apos;] == &quot;on&quot;) {
				departDate = LocalDate.parse(buffer_departDate, formatter)
				daysToAdd = random.nextInt(10) + 1
				returnDate = departDate.plusDays(daysToAdd)
				returnDate = returnDate.format(formatter)
			} else {
				returnDate = docJsoup.select(&quot;input[name=&apos;returnDate&apos;]&quot;).attr(&quot;value&quot;)
			}
			
			log.info(&quot;returnDate = ${returnDate}&quot;)
			requestData[key] = returnDate
//			vars.put(key, result)
			break
		case &apos;numPassengers&apos;:
			result = docJsoup.select(&quot;input[name=&apos;numPassengers&apos;]&quot;).attr(&quot;value&quot;)
			log.info(&quot;numPassengers = ${result}&quot;)
			requestData[key] = result
//			vars.put(key, result)
			break
		case &apos;seatPref&apos;:
//			result = requestData[key][random.nextInt(3)]
			list_seatPref = docJsoup.select(&quot;input[name=&apos;seatPref&apos;]&quot;)
						.collect { it.attr(&quot;value&quot;) }
			if (list_seatPref.isEmpty()) {
				log.warn(&quot;Oops, в list_seatPref нет елементов для выбора.&quot;)
				break
			}
			result = list_seatPref[random.nextInt(list_seatPref.size())]
			log.info(&quot;Случайно выбранный параметр seatPref: ${result}&quot;)
			requestData[key] = result
//			vars.put(key, result)
			break
		case &apos;seatType&apos;:
//			result = requestData[key][random.nextInt(3)]
			list_seatType = docJsoup.select(&quot;input[name=&apos;seatType&apos;]&quot;)
							.collect { it.attr(&quot;value&quot;) }
			if (list_seatType.isEmpty()) {
				log.warn(&quot;Oops, в list_seatType нет елементов для выбора.&quot;)
				break
			}
			result = list_seatType[random.nextInt(list_seatType.size())]
			log.info(&quot;Случайно выбранный параметр seatType: ${result}&quot;)
			requestData[key] = result
//			vars.put(key, result)
			break
		default:
			println(&quot;Unknown fruit.&quot;)
	}
}

// Сохраняем значение в переменные JMeter
requestData.each {
	key, value -&gt; vars.put(key, value)
}
// Обозначаем в логах завершение скрипта
log.info(&quot;STOP PostProcessor Script...&quot;)
return(0)
</stringProp>
