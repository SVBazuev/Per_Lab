              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">/**
 *  Скрипт извлекает данные из FirstLastEnNames.xml
 *  и создаёт в области JMeter следующие переменные:
 *      —  &apos;firstName&apos; ;
 *      —  &apos;lastName&apos; ;
 *      —  &apos;username&apos; ;
 *      —  &apos;password&apos; ;
 *      —  &apos;address1&apos; ;
 *      —  &apos;address2&apos; ;
 *      —  &apos;pass1&apos; ;
 *      —  &apos;pass*&apos; * (n-1) .
 *  Параметры:
 *            args[0]: процент покупки билета на доного пассажира,  
 *                     если 100 — всегда один ; 
 *                     если 0  — всегда два ; 
 *                     если 75  — в 25% случаев более одного пассажира. 
 *  
 *            args[1]: dataSet (FirstLastEnNames.xml)
 *  
 *            args[2]: регистрационные данные сохраняются в NewUsersData.csv
 *                     в формате —  NickName;Password;FullName
 * 
 *  return(0)
 */


import java.util.Random
import groovy.xml.XmlSlurper


def generatePassword(length) {
    chars = &apos;abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789!@#$^&amp;()&apos;
    random = new Random()
    password = (1..length).collect { chars[random.nextInt(chars.length())] }.join(&apos;&apos;)
    return password
}

// Обозначаем в логах старт скрипта
log.info(&quot;START genNewUser Script...&quot;)


random = new Random()
chance = random.nextInt(100)

procent = Integer.parseInt(args[0])
if (procent == 100) {
	numPass = 1
} else if (procent == 0) {
	numPass = 2
} else {
	numPass = chance &gt;= procent ? [3, 4][random.nextInt(2)] : 1
}
vars.put(&quot;numPassengers&quot;, &quot;${numPass}&quot;)


baseDir = new File(org.apache.jmeter.services.FileServer.getFileServer().getBaseDir())
			.getAbsolutePath()
fileSep = File.separator

fileIn = &quot;${args[1]}&quot;

fileOut = &quot;${args[2]}&quot;

absPathIn = /${baseDir}${fileSep}${fileIn}/
absPathOut = /${baseDir}${fileSep}${fileOut}/

xml = new XmlSlurper().parse(new File(absPathIn))
csv = new File(absPathOut)
if (!csv.exists() || csv.length() &lt; 27) {
    // Если файл не существует или пуст, создаем его и записываем заголовки
    csv.withWriter(&apos;UTF-8&apos;) { writer -&gt;
        writer.writeLine(&quot;NickName;Password;FullName&quot;)
    }
}


sex = [&quot;FemaleNames&quot;, &quot;MaleNames&quot;]
selectedSex = sex[random.nextInt(sex.size())]
FirstStartWith = selectedSex == &quot;FemaleNames&quot; ? 
	xml.FirstNames.FemaleNames.StartWith.collect { it.@letter }
	:
	xml.FirstNames.MaleNames.StartWith.collect { it.@letter }

LastStartWith = xml.LastNames.StartWith.collect { it.@letter }

for (i=1; i&lt;=numPass; i++) {
	selectedSex = sex[random.nextInt(sex.size())]
	randomFirstStartWithLetter = FirstStartWith[random.nextInt(FirstStartWith.size())]
	randomLastStartWithLetter = LastStartWith[random.nextInt(LastStartWith.size())]
	
	firstNames = selectedSex == &quot;FemaleNames&quot; ?
		xml.FirstNames.FemaleNames.StartWith.find { 
			it.@letter == randomFirstStartWithLetter 
		}.FirstName
		: xml.FirstNames.MaleNames.StartWith.find { 
			it.@letter == randomFirstStartWithLetter
		}.FirstName
	
	first_name = firstNames[random.nextInt(firstNames.size())].toString()
	
	lastNames = xml.LastNames.StartWith.find { 
			it.@letter == randomLastStartWithLetter 
		}.LastName
	
	last_name = lastNames[random.nextInt(lastNames.size())].toString()

	if (i == 1) {
		nickname = (first_name.substring(0,3) + last_name.substring(0,3)).toLowerCase()
		password = generatePassword(8)
		csv.withWriterAppend(&apos;UTF-8&apos;) { writer -&gt;
			writer.writeLine(&quot;${nickname};${password};${first_name} ${last_name}&quot;)
		}
		vars.put(&apos;username&apos;, nickname)
		vars.put(&apos;password&apos;, password)
		vars.put(&quot;firstName&quot;, first_name)
		vars.put(&quot;lastName&quot;, last_name)
		vars.put(&quot;address1&quot;, &quot;not Street not Address&quot;)
		vars.put(&quot;address2&quot;, &quot;Izhevsk%2FCCCP%2F426000&quot;)
	}
	vars.put(&quot;pass${i}&quot;, &quot;${first_name} ${last_name}&quot;)
	log.info(&quot;pass${i} == ${first_name} ${last_name}&quot;)
}

// Обозначаем в логах завершение скрипта
log.info(&quot;STOP genNewUser Script...&quot;)
return(0)
